<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zyh.deviceinfo">

    <uses-permission android:name="android.permission.READ_FRAME_BUFFER" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CPUGPUInfoActivity"
            android:label="CPU&amp;GPUInfo" />
        <activity
            android:name=".TestGPUActivity"
            android:label="Detecting GPU..." />
        <activity android:name=".CPUGPUStatusActivity"
            android:label="CPU&amp;GPUStatus"></activity>
    </application>

</manifest>